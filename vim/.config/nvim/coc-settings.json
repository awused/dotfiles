{
  "diagnostic.checkCurrentLine": true,
  "rust-analyzer.imports.granularity.group": "module",
  "rust-analyzer.checkOnSave.command": "clippy",
  "rust-analyzer.checkOnSave.extraArgs": ["--", "-Wclippy::nursery",
    "-Wunsafe_op_in_unsafe_fn",

    "-Wclippy::borrow_as_ptr",
    "-Wclippy::cloned_instead_of_copied",
    "-Wclippy::default_trait_access",
    "-Wclippy::derive-partial-eq-without-eq",
    "-Wclippy::explicit_iter_loop",
    "-Wclippy::explicit_into_iter_loop",
    "-Wclippy::fn_to_numeric_cast_any",
    "-Wclippy::implicit_clone",
    "-Wclippy::implicit_saturating_sub",
    "-Wclippy::is_digit_ascii_radix",
    "-Wclippy::let_underscore_drop",
    "-Wclippy::let_underscore_must_use",
    "-Wclippy::let_unit_value",
    "-Wclippy::manual_assert",
    "-Wclippy::manual_let_else",
    "-Wclippy::map_flatten",
    "-Wclippy::map_unwrap_or",
    "-Wclippy::match_same_arms",
    "-Wclippy::match_wildcard_for_single_variants",
    "-Wclippy::missing_panics_doc",
    "-Wclippy::missing_const_for_fn",
    "-Wclippy::module_name_repetitions",
    "-Wclippy::must_use_candidate",
    "-Wclippy::needless_late_init",
    "-Wclippy::obfuscated_if_else",
    "-Wclippy::ptr_as_ptr",
    "-Wclippy::redundant_closure_for_method_calls",
    "-Wclippy::redundant_else",
    "-Wclippy::return_self_not_must_use",
    "-Wclippy::single_match_else",
    "-Wclippy::string_to_string",
    "-Wclippy::trim_split_whitespace",
    "-Wclippy::trivially_copy_pass_by_ref",
    "-Wclippy::type_repetition_in_bounds",
    "-Wclippy::uninlined_format_args",
    "-Wclippy::unnecessary_to_owned",
    "-Wclippy::unnested_or_patterns",
    "-Wclippy::unused_self",
    "-Wclippy::unreadable_literal",
    "-Wclippy::use_self",
    "-Wclippy::wildcard_enum_match_arm",


    "-Aclippy::await_holding_refcell_ref",
    "-Aclippy::future_not_send",
    "-Aclippy::iter_on_empty_collections",
    "-Aclippy::module-name-repetitions",
    "-Aclippy::needless_collect",
    "-Aclippy::option_if_let_else",
    "-Aclippy::suspicious_operation_groupings"
  ],
  "rust-analyzer.procMacro.enable": true,
  "rust-analyzer.procMacro.attributes.enable": true,
  "rust-analyzer.cargo.features": [],
  "rust-analyzer.cargo.loadOutDirsFromCheck": true,
  "rust-analyzer.cargo.buildScripts.enable": true
}
