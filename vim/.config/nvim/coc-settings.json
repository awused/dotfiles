{
  "diagnostic.checkCurrentLine": true,
  "rust-analyzer.assist.importGranularity": "module",
  "rust-analyzer.checkOnSave.command": "clippy",
  "rust-analyzer.checkOnSave.extraArgs": ["--", "-Wclippy::nursery",
    "-Aclippy::future_not_send", "-Wclippy::match_same_arms",
    "-Wclippy::redundant_closure_for_method_calls",
    "-Wclippy::let_unit_value", "-Wclippy::map_unwrap_or",
    "-Wclippy::let_underscore_drop", "-Wclippy::unused_self",
    "-Wclippy::single_match_else", "-Wclippy::trivially_copy_pass_by_ref",
    "-Wclippy::redundant_else", "-Wclippy::module_name_repetitions",
    "-Wclippy::unreadable_literal", "-Wclippy::missing_panics_doc",
    "-Wclippy::default_trait_access", "-Wclippy::implicit_saturating_sub",
    "-Wclippy::unnested_or_patterns", "-Wclippy::must_use_candidate",
    "-Wclippy::use_self", "-Wclippy::missing_const_for_fn",
    "-Wclippy::type_repetition_in_bounds", "-Aclippy::branches_sharing_code",
    "-Aclippy::module-name-repetitions", "-Aclippy::needless_collect",
    "-Wclippy::map_flatten", "-Aclippy::option_if_let_else",
    "-Wunsafe_op_in_unsafe_fn", "-Wclippy::fn_to_numeric_cast_any",
    "-Wclippy::unnecessary_to_owned",
    "-Wclippy::needless_late_init", "-Wclippy::return_self_not_must_use",
    "-Aclippy::await_holding_refcell_ref", "-Wclippy::manual_assert",
    "-Wclippy::explicit_iter_loop", "-Wclippy::explicit_into_iter_loop",
    "-Wclippy::implicit_clone", "-Wclippy::needless_collect",
    "-Wclippy::match_wildcard_for_single_variants",
    "-Wclippy::borrow_as_ptr", "-Wclippy::cloned_instead_of_copied",
  ],
  "rust-analyzer.procMacro.enable": true,
  "rust-analyzer.cargo.allFeatures": true,
  "rust-analyzer.cargo.loadOutDirsFromCheck": true,
  "rust-analyzer.cargo.runBuildScripts": true
}
